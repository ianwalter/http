{
  "name": "@ianwalter/http",
  "version": "1.2.0",
  "description": "A transpiled global instance of ky (the fetch-based HTTP client)",
  "main": "dist/http.js",
  "umd:main": "dist/http.umd.js",
  "module": "dist/http.m.js",
  "scripts": {
    "bundle": "microbundle build index.js --external none --target browser --no-sourcemap --no-minify",
    "transpile": "babel dist/*.js -d dist --minified --source-maps",
    "build": "npm run bundle && npm run transpile",
    "watch": "microbundle watch index.js --external none",
    "test": "npm run build && jest",
    "lint": "eslint --ext js index.js tests"
  },
  "keywords": [
    "http",
    "fetch",
    "ky"
  ],
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianwalter/http.git"
  },
  "bugs": {
    "url": "https://github.com/ianwalter/http/issues"
  },
  "homepage": "https://github.com/ianwalter/http#readme",
  "dependencies": {
    "ky": "^0.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "fetch-mock": "^7.2.5",
    "jest": "^23.6.0",
    "jest-puppeteer": "^3.7.0",
    "microbundle": "^0.9.0",
    "np": "^3.0.4",
    "puppeteer": "^1.11.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "jest/globals": true
    },
    "extends": [
      "standard"
    ],
    "plugins": [
      "jest"
    ],
    "globals": {
      "page": true,
      "fetchMock": true,
      "http": true
    }
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "preset": "jest-puppeteer"
  }
}
