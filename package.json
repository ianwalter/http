{
  "name": "@ianwalter/http",
  "version": "2.0.0",
  "description": "A transpiled global instance of ky (the fetch-based HTTP client)",
  "main": "dist/http.js",
  "module": "dist/http.m.js",
  "iife": "dist/http.iife.js",
  "scripts": {
    "lint": "eslint index.js tests",
    "dist": "dist --inline ky --babel --no-iife",
    "iife": "dist --inline --babel --no-cjs --no-esm",
    "pretest": "npm run dist && npm run iife",
    "test": "ava"
  },
  "keywords": [
    "http",
    "fetch",
    "ky"
  ],
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianwalter/http.git"
  },
  "bugs": {
    "url": "https://github.com/ianwalter/http/issues"
  },
  "homepage": "https://github.com/ianwalter/http#readme",
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "ky": "^0.5.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@ianwalter/dist": "^1.5.1",
    "@ianwalter/puppeteer-helper": "^1.0.0",
    "ava": "^1.2.0",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "fetch-mock": "^7.2.5",
    "np": "^3.0.4"
  },
  "browserslist": [
    "> 0.5%"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "jest/globals": true
    },
    "extends": [
      "eslint:recommended",
      "standard"
    ],
    "plugins": [
      "jest"
    ],
    "globals": {
      "page": true,
      "fetchMock": true,
      "http": true
    }
  }
}
