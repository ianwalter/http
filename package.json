{
  "name": "@ianwalter/http",
  "version": "1.2.0",
  "description": "A transpiled global instance of ky (the fetch-based HTTP client)",
  "main": "dist/http.js",
  "module": "dist/http.m.js",
  "browser": "dist/http.browser.js",
  "scripts": {
    "lint": "eslint index.js tests",
    "transpile": "babel index.js -o dist/http.m.js",
    "pretest": "mkdir -p dist && npm run transpile && dist --inline",
    "test": "jest"
  },
  "keywords": [
    "http",
    "fetch",
    "ky"
  ],
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianwalter/http.git"
  },
  "bugs": {
    "url": "https://github.com/ianwalter/http/issues"
  },
  "homepage": "https://github.com/ianwalter/http#readme",
  "dependencies": {
    "ky": "^0.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "@ianwalter/dist": "^1.0.0-1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "fetch-mock": "^7.2.5",
    "jest": "^23.6.0",
    "jest-puppeteer": "^3.7.0",
    "np": "^3.0.4",
    "puppeteer": "^1.11.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "jest/globals": true
    },
    "extends": [
      "standard"
    ],
    "plugins": [
      "jest"
    ],
    "globals": {
      "page": true,
      "fetchMock": true,
      "http": true
    }
  },
  "browserslist": [
    "> 0.25%"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
  "jest": {
    "preset": "jest-puppeteer"
  }
}
